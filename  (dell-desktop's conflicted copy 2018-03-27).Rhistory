get_pages<-function(nickname=NULL,topic=NULL,category=NULL) {
criteria=c()
if (!is.null(topic))
criteria["topic"]<-topic
if (!is.null(category))
criteria["category"]<-category
files<-get_files()
sfiles<-searchable(files)
res<-lookup(sfiles,criteria)
res
}
